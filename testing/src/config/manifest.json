{
    "env": {
        "doc": "The environment to run in",
        "format": [
            "production",
            "development",
            "test"
        ],
        "default": "development",
        "env": "NODE_ENV"
    },
    "service": {
        "uri": {
            "doc": "The base URI for the Service",
            "format": "url",
            "default": "http://localhost:3000",
            "env": "SERVICE_URI"
        }
    }
}
