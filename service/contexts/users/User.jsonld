{
    "@context": {
        "schema": "http://schema.org/",
        "worlds": "http://worlds/jsonld/vocab.jsonld#",

        "Person": "schema:Person",
        "User": "worlds:User",
        "Login": "worlds:Login",

        "created": "schema:dateCreated",
        "updated": "schema:dateModified",
        "name": "schema:name",
        "email": "schema:email",
        "logins": {
            "@id": "worlds:User/logins",
            "@container": "@set"
        },
        "provider": "worlds:Login/provider",
        "providerId": "worlds:Login/providerId",
        "providerName": "worlds:Login/providerName"
    }
}
